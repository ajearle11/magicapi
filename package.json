{
  "name": "magicapi",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "setup-db": "node ./database/setup-db.js",
    "dev": "nodemon -L index.js",
    "coverage": "jest --coverage",
    "test": "jest --watchAll=false --reporters=default --reporters=jest-junit"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
