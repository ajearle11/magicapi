    "testPathIgnorePatterns": [
      "/__tests__/db/config.js"
    ]
  },
  "jest-junit": {
    "suiteNameTemplate": "./__tests__/api.spec.js",
    "outputDirectory": ".",
    "outputName": "junit.xml"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jest": "^29.6.4",
    "morgan": "^1.10.0",
    "nodemon": "^3.0.1",
    "pg": "^8.11.3",
    "pg-pool": "^3.6.1",
    "supertest": "^6.3.3"
  },
  "devDependencies": {
    "jest-junit": "^16.0.0"
  }
}
